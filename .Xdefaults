URxvt*urgentOnBell:     true
URxvt.scrollBar:        false
URxvt.perl-ext-common:  default,matcher,tabbed
URxvt.urlLauncher:      firefox
urxvt.font:             xft:Monospace:style=Regular:pixelsize=12:antialias=true
urxvt.boldFont:         xft:Monospace:style=Bold:pixelsize=12:antialias=true
URxvt*shading:          20
URxvt*fading:           10

# colors
URxvt.background:       #1d1d1d
URxvt.foreground:       #bfbf97

# red
*color1:                #cc0000
*color9:                #ef5919

# green
*color2:                #00cc00
*color10:                #19ef19

# blue
*color4:                #0000cc
*color12:               #2919ef

# Magenta
*color5:                #cc00cc
*color13:               #df19df
